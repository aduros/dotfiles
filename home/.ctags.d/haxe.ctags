--langdef=haxe
--langmap=haxe:.hx
--regex-haxe=/^[ \t]*[(private|public|static|override|inline|dynamic)( \t)]*function[ \t]+([A-Za-z0-9_]+)[ \t]*\(([^\{]*)/\1/f,functions/
--regex-haxe=/^[ \t]*([private|public|static|protected|inline][ \t]?)+var[ \t]+([A-Za-z0-9_]+)/\2/v,variables/ 
--regex-haxe=/\bclass\s+([A-Za-z0-9_]+)/\1/c,classes/
--regex-haxe=/\binterface\s+([A-Za-z0-9_]+)/\1/i,interfaces/
--regex-haxe=/\benum\s+([A-Za-z0-9_]+)/\1/e,enums/
--regex-haxe=/\btypedef\s+([A-Za-z0-9_]+)\s*=/\1/t,typedefs/
--regex-haxe=/\babstract\s+([A-Za-z0-9_]+)/\1/a,abstracts/
